{
  "build": {
    "devPath": "../pages",
    "distDir": "../pages",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "VolleyGroups",
    "version": "0.0.0"
  },
  "tauri": {
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": [
        "https://volleygroups.ninju.dev/api/update"
      ],
      "pubkey": "",
      "windows": {
        "installMode": "passive",
        "installerArgs": []
      }
    },
    "allowlist": {
      "all": false,
      "shell": {
        "all": false ,
        "open": true
      }
    },
    "windows": [
      {
        "title": "VolleyGroups",
        "width": 700,
        "height": 400,
        "resizable": false,
        "fullscreen": false,
        "decorations": false,
        "transparent": true,
        "visible": true,
        "label": "main"
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "VolleyGroups",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
